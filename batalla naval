//Juego
printf("Bienvenido al juego, intenta hundir los barcos de tu rival\n");


do{ 
  while (jugador==1){ 
    for(i=0;i<10;i++){
      for(j=0;j<10;j++){
        printf("%c  ", taene1[i][j]);
      }
      printf("\n");
    }
    printf("Turno del jugador 1:\n");
    printf("Elija numero de fila\n");
    scanf("%i", &f);
    printf("Elija numero de columna\n");
    scanf("%i", &c);
    if(tapro2[f-1][c-1]=='X'){
      taene1[f-1][c-1]='X';
      if(tapro2[f-1][c]!='X' && tapro2[f-1][c-2]!='X'){
        if(tapro2[f][c-1]!='X' && tapro2[f-2][c-1]!='X'){
          taene1[f-1][c-1]='*';
          hundido1++;
          printf("Has hundido una fragata\n");
        }
      }else if(tapro2[f-1][c]=='X' || tapro2[f-1][c-2]=='X'){
        if(tapro2[f-1][c]=='X' && tapro2[f-1][c+1]!='X'){
          if(tapro2[f-1][c-2]!='X'){ 
            if(taene1[f-1][c]=='X'){
              taene1[f-1][c-1]='*';
              taene1[f-1][c]='*';
              hundido1++;
              printf("Has hundido un destructor\n");
            } 
          }
        }
        if(tapro2[f-1][c-2]=='X' && tapro2[f-1][c-3]!='X'){
          if(tapro2[f-1][c]!='X')
          if(taene1[f-1][c-2]=='X'){
            taene1[f-1][c-1]='*';
            taene1[f-1][c-2]='*';
            hundido1++;
            printf("Has hundido un destructor\n");
          }
        }
      }
      if(tapro2[f][c-1]=='X' || tapro2[f-2][c-1]=='X'){
        if(tapro2[f][c-1]=='X' && tapro2[f-2][c-1]!='X'){
          if(tapro2[f+1][c-1]!='X'){ 
            if(taene1[f][c-1]=='X'){
              taene1[f-1][c-1]='*';
              taene1[f][c-1]='*';
              hundido1++;
              printf("Has hundido un destructor\n");
            } 
          }
        }
        if(tapro2[f-2][c-1]=='X' && tapro2[f-3][c-1]!='X'){
          if(tapro2[f][c-1]!='X'){ 
            if(taene1[f-2][c-1]=='X'){
              taene1[f-1][c-1]='*';
              taene1[f-2][c-1]='*';
