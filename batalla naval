#include <stdio.h>

int main(void) {

  char tapro1[10][10], taene1[10][10], tapro2[10][10], taene2[10][10];
  int i, j, x=0, cf=2, ca=3, cd=3, cp=1, f=0, c=0, jugador=1, hundido1=0, hundido2=0;

  for(i=0;i<10;i++){
    for(j=0;j<10;j++){
      tapro1[i][j]='O';
      taene1[i][j]='O';
    }
  }

  for(i=0;i<10;i++){
    for(j=0;j<10;j++){
      tapro2[i][j]='O';
      taene2[i][j]='O';
    }
  }
 
//Jugador 1
  for(i=0;i<9;i++){ 
    for(int k=0;k<10;k++){
      for(j=0;j<10;j++){
        printf("%c  ", tapro1[k][j]);
      }
      printf("\n");
    }
    printf("Que barco desea acomodar\n");
    if(cf!=0){ 
      printf("Opcion 1: Fragata = 1 casilla\n");
    }
    if(cd!=0){ 
      printf("Opcion 2: Destructor = 2 casilla\n");
    }
    if(ca!=0){ 
      printf("Opcion 3: Acorazado = 3 casilla\n");
    }
    if(cp!=0){ 
      printf("Opcion 4: Portaaviones = 4 casilla\n");
    }
    scanf("%i",&x);
 
//Fragatas
    if(x==1 && cf!=0){
      do { 
      printf("Donde desea acomodar la fragata, elija numero de fila\n");
      scanf("%i", &f);
      printf("Donde desea acomodar la fragata, elija numero de columna\n");
      scanf("%i", &c);
      if(tapro1[f-1][c-1]!='X' && tapro1[f-1][c-1]!='.'){
        if(c==1){
          tapro1[f-1][c-1]='X';
          tapro1[f][c-1]='.';
          tapro1[f-1][c]='.';
          tapro1[f-2][c-1]='.';
          tapro1[f][c]='.';
          tapro1[f-2][c]='.';
        } else if(c==10){
          tapro1[f-1][c-1]='X';
          tapro1[f][c-1]='.';
          tapro1[f-2][c-1]='.';
          tapro1[f-1][c-2]='.';
          tapro1[f-2][c-2]='.';
          tapro1[f][c-2]='.';
        }else{
          tapro1[f-1][c-1]='X';
          tapro1[f][c-1]='.';
          tapro1[f-1][c]='.';
          tapro1[f-2][c-1]='.';
          tapro1[f-1][c-2]='.';
          tapro1[f-2][c-2]='.';
          tapro1[f][c]='.';
          tapro1[f][c-2]='.';
          tapro1[f-2][c]='.';
        }
        cf--;
        printf("Fragatas restantes: %i\n", cf);
        x=0; 
      }else{
        printf("Aqui hay un barco, vuelve a elegir\n");
      }
      }while(x!=0);
    } 
//Fragatas
//Destructores
    if(x==2 && cd!=0){
      do{ 
      printf("Donde desea acomodar el destructor, elija numero de fila\n");
      scanf("%i", &f);
      printf("Donde desea acomodar el destructor, elija numero de columna\n");
