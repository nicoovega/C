    if(tapro1[f-1][c-1]=='X'){
      taene2[f-1][c-1]='X';
      if(tapro1[f-1][c]!='X' && tapro1[f-1][c-2]!='X'){
        if(tapro1[f][c-1]!='X' && tapro1[f-2][c-1]!='X'){
          taene2[f-1][c-1]='*';
          hundido2++;
