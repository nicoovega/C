              taene1[f-2][c-1]='*';
              hundido1++;
              printf("Has hundido un acorazado\n");
            } 
          } 
        }
      }
      if(tapro2[f-1][c]=='X' || tapro2[f-1][c-2]=='X' || tapro2[f-1][c+1]=='X' || tapro2[f-1][c-3]=='X' || tapro2[f-1][c+2]=='X' || tapro2[f-1][c-4]=='X'){
