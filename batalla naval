          if(tapro2[f-4][c-1]!='X' && tapro2[f][c-1]!='X'){
            if(taene1[f-2][c-1]=='X' && taene1[f-3][c-1]=='X'){
              taene1[f-1][c-1]='*';
              taene1[f-2][c-1]='*';
              taene1[f-3][c-1]='*';
              hundido1++;
              printf("Has hundido un acorazado\n");
            } 
          } 
        }
        if(tapro2[f][c-1]=='X' && tapro2[f-2][c-1]=='X'){
          if(tapro2[f+1][c-1]!='X' && tapro2[f-3][c-1]!='X'){
            if(taene1[f][c-1]=='X' && taene1[f-2][c-1]=='X'){
