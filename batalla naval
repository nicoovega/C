              tapro1[f-1][c]='X';
              tapro1[f][c-1]='.';
              tapro1[f-2][c-1]='.';
              tapro1[f-1][c-2]='.';
              tapro1[f-2][c-2]='.';
              tapro1[f][c]='.';
