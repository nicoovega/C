      if(tapro2[f][c-1]=='X' || tapro2[f+1][c-1]=='X' || tapro2[f-2][c-1]=='X' || tapro2[f-3][c-1]=='X' || tapro2[f-4][c-1]=='X' || tapro2[f+2][c-1]=='X'){ 
