      printf("Opcion 1: Derecha\n");
      printf("Opcion 2: Abajo\n");
      scanf("%i", &x);
      if(c-1==9 && x==1){ 
        printf("No se puede colocar un barco en esta direccion\n");
      }else{
        if(x==1){
          if(tapro1[f-1][c]!='X' && tapro1[f-1][c]!='.'){ 
            if(c==1){
              tapro1[f-1][c-1]='X';
              tapro1[f-1][c]='X';
              tapro1[f][c-1]='.';
