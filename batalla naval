        }
      }
       if(tapro2[f-1][c]=='X' || tapro2[f-1][c-2]=='X' || tapro2[f-1][c+1]=='X' || tapro2[f-1][c-3]=='X'){
        if(tapro2[f-1][c]=='X' && tapro2[f-1][c+1]=='X'){
          if(tapro2[f-1][c+2]!='X' && tapro2[f-1][c-2]!='X'){
            if(taene1[f-1][c]=='X' && taene1[f-1][c+1]=='X'){
              taene1[f-1][c-1]='*';
              taene1[f-1][c]='*';
              taene1[f-1][c+1]='*';
              hundido1++;
              printf("Has hundido un acorazado\n");
            }
          }
        }
        if(tapro2[f-1][c-2]=='X' && tapro2[f-1][c-3]=='X'){
          if(tapro2[f-1][c-4]!='X' && tapro2[f-1][c]!='X'){
            if(taene1[f-1][c-2]=='X' && taene1[f-1][c-3]=='X'){
              taene1[f-1][c-1]='*';
              taene1[f-1][c-2]='*';
              taene1[f-1][c-3]='*';
              hundido1++;
              printf("Has hundido un acorazado\n");
            }
          }
        }
        if(tapro2[f-1][c-2]=='X' && tapro2[f-1][c]=='X'){
          if(tapro2[f-1][c-3]!='X' && tapro2[f-1][c+1]!='X'){
            if(taene1[f-1][c-2]=='X' && taene1[f-1][c]=='X'){
              taene1[f-1][c-1]='*';
              taene1[f-1][c-2]='*';
              taene1[f-1][c]='*';
              hundido1++;
              printf("Has hundido un acorazado\n");
            }
          }
        } 
